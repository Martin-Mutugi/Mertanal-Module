<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Data for <%= table %></title>
</head>
<body>
  <h1>Data from <%= table.replace('table', 'Table ') %></h1>

  <ul>
    <% data.forEach(function(entry) { %>
      <li>
        <pre><%= JSON.stringify(entry, null, 2) %></pre>
        <a href="/edit/<%= table %>/<%= entry.id %>">Edit</a> |
        <a href="/delete/<%= table %>/<%= entry.id %>">Delete</a>
      </li>
    <% }) %>
  </ul>
</body>
</html>
